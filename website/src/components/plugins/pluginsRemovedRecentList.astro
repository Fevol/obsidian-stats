---
import { getPluginRemovedRecentList } from '../../../../src/plugin/data';
import PluginLink from '../pluginLink.astro';
import Commit from '../commit.astro';
import { getPluginData } from '../../../../src/utils';

const retiredPlugins = getPluginRemovedRecentList(getPluginData());
---

<table>
	<thead>
		<tr>
			<th>Plugin</th>
			<th>Removed Date</th>
		</tr>
	</thead>
	<tbody>
		{
			retiredPlugins.map(x => (
				<tr>
					<td>
						<PluginLink id={x.id} />
					</td>
					<td>
						<Commit commit={x.removedCommit} />
					</td>
				</tr>
			))
		}
	</tbody>
</table>
