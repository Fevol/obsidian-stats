---
import { getTimeBetweenMajorReleases, getTimeBetweenReleases } from "../../../../src/release/data";
import ReleaseDataFile from '../../../../releases-full-data.json' assert { type: 'json' };
import ReleaseTimeBetween from '../svelte/releaseTimeBetween.svelte';

const regularDataPoints = getTimeBetweenReleases(ReleaseDataFile);
const majorDataPoints = getTimeBetweenMajorReleases(ReleaseDataFile);
---

