---
import PluginComponent from '../../components/plugin.astro';
import { getPluginData } from '../../../../src/utils';

export function getStaticPaths() {
	const pluginData = getPluginData();
	return pluginData.map(plugin => {
		return {
			params: {
				plugin: plugin.id,
			},
			props: {
				pluginDatum: plugin,
			},
		};
	});
}

const { pluginDatum } = Astro.props;
---

<PluginComponent plugin={pluginDatum} />
